{
    "name": "myhdfw-frontend-vite",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:e2e:integration:chromium": "testcafe chromium ./src/integrationtest/",
        "test:e2e:integration:chrome": "testcafe chrome ./src/integrationtest/",
        "test:e2e:system:chromium": "testcafe chromium ./src/systemtest/",
        "test:e2e:system:chrome": "testcafe chrome ./src/systemtest/",
        "pregenerate-api:linux": "if [ -d src/api/service ]; then rm -rf src/api/service; fi",
        "pregenerate-api:windows": "if exist src\\api\\service rmdir /s /q src\\api\\service",
        "generate-api": "openapi-generator-cli generate -g typescript-axios -i ./src/api/api-docs.yaml -o src/api/service --skip-validate-spec"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.10",
        "@fullcalendar/daygrid": "^6.1.10",
        "@fullcalendar/interaction": "^6.1.10",
        "@fullcalendar/list": "^6.1.10",
        "@fullcalendar/timegrid": "^6.1.10",
        "@fullcalendar/vue3": "^6.1.10",
        "axios": "^1.6.7",
        "bootstrap-vue": "^2.23.1",
        "event-utils": "^2.1.0",
        "primeflex": "^3.0.1",
        "primeicons": "^6.0.1",
        "primevue": "^3.46.0",
        "vue": "^3.3.11",
        "vue-jwt-decode": "^0.1.0",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.23.3",
        "@openapitools/openapi-generator-cli": "^2.11.0",
        "@vitejs/plugin-vue": "^4.5.2",
        "@vitest/coverage-istanbul": "^1.3.1",
        "@vitest/ui": "^1.3.1",
        "@vue/test-utils": "^2.4.3",
        "jsdom": "^23.0.1",
        "sass": "^1.70.0",
        "testcafe": "^3.5.0",
        "vite": "^5.0.10",
        "vitest": "^1.3.1"
    }
}
